<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>ai.mollendorff.daneel-reset</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/sh</string>
		<string>${DANEEL_HOME}/bin/daily-reset.sh</string>
	</array>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>4</integer>
		<key>Minute</key>
		<integer>0</integer>
	</dict>
	<key>StandardOutPath</key>
	<string>${DANEEL_HOME}/log/reset.log</string>
	<key>StandardErrorPath</key>
	<string>${DANEEL_HOME}/log/reset.log</string>
	<key>WorkingDirectory</key>
	<string>${DANEEL_HOME}</string>
</dict>
</plist>
